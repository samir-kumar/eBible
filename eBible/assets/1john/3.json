[{"end_ref_id":null,"ref_title":"4:8","start_ref_id":62004008,"verse_id":62003010,"letter_id":"t"}],"c62003010.2":[{"end_ref_id":62004021,"ref_title":"4:20-21","start_ref_id":62004020,"verse_id":62003010,"letter_id":"u"}],"c62003011.1":[{"end_ref_id":null,"ref_title":"1:5","start_ref_id":62001005,"verse_id":62003011,"letter_id":"v"},{"end_ref_id":null,"ref_title":"2:24","start_ref_id":62002024,"verse_id":62003011,"letter_id":"v"}],"c62003021.1":[{"end_ref_id":null,"ref_title":"1 Cor. 4:4","start_ref_id":46004004,"verse_id":62003021,"letter_id":"k"}]},"foot_notes":{"n62003002.1":{"note_class":"alternative","note_sub_class":null,"content":"Or <i><span class='catch-word'>when</span> it appears</i>","verse_id":62003002},"n62003013.1":{"note_class":"translation","note_sub_class":"gender-neutral","content":"Or <i><span class='catch-word'>brothers</span> and sisters</i>. The plural Greek word <i language='Greek'>adelphoi</i> (translated &ldblquot;brothers&rdblquot;) refers to siblings in a family. In New Testament usage, depending on the context, <i language='Greek'>adelphoi</i> may refer either to men or to both men and women who are siblings (brothers and sisters) in God&apos;s family, the church; also verses 14, 16","verse_id":62003013},"n62003024.1":{"note_class":"translation","note_sub_class":"original","content":"Greek <i>him</i>","verse_id":62003024},"n62003024.2":{"note_class":"translation","note_sub_class":"original","content":"Greek <i>he</i>","verse_id":62003024},"n62003009.1":{"note_class":"translation","note_sub_class":"original","content":"Greek <i>his</i>","verse_id":62003009}}}}