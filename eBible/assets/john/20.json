t_ref_id":44002025,"verse_id":43020009,"letter_id":"n"},{"end_ref_id":44013035,"ref_title":"13:34-35","start_ref_id":44013034,"verse_id":43020009,"letter_id":"n"},{"end_ref_id":null,"ref_title":"17:3","start_ref_id":44017003,"verse_id":43020009,"letter_id":"n"},{"end_ref_id":null,"ref_title":"1 Cor. 15:4","start_ref_id":46015004,"verse_id":43020009,"letter_id":"n"}],"c43020013.2":[{"end_ref_id":null,"ref_title":"2","start_ref_id":43020002,"verse_id":43020013,"letter_id":"r"}],"c43020014.1":[{"end_ref_id":null,"ref_title":"Mark 16:9","start_ref_id":41016009,"verse_id":43020014,"letter_id":"s"},{"end_ref_id":null,"ref_title":"Matt. 28:9","start_ref_id":40028009,"verse_id":43020014,"letter_id":"s"}],"c43020025.1":[{"end_ref_id":null,"ref_title":"20","start_ref_id":43020020,"verse_id":43020025,"letter_id":"p"},{"end_ref_id":null,"ref_title":"Ps. 22:16","start_ref_id":19022016,"verse_id":43020025,"letter_id":"p"}],"c43020031.4":[{"end_ref_id":43003016,"ref_title":"3:15-16","start_ref_id":43003015,"verse_id":43020031,"letter_id":"z"},{"end_ref_id":null,"ref_title":"5:40","start_ref_id":43005040,"verse_id":43020031,"letter_id":"z"},{"end_ref_id":null,"ref_title":"6:53","start_ref_id":43006053,"verse_id":43020031,"letter_id":"z"},{"end_ref_id":null,"ref_title":"10:10","start_ref_id":43010010,"verse_id":43020031,"letter_id":"z"}]},"foot_notes":{"n43020024.1":{"note_class":"translation","note_sub_class":"original","content":"Greek <i>Didymus</i>","verse_id":43020024},"n43020016.1":{"note_class":"alternative","note_sub_class":null,"content":"Or <i>Hebrew</i>","verse_id":43020016},"n43020007.1":{"note_class":"translation","note_sub_class":"original","content":"Greek <i>his</i>","verse_id":43020007}}}}